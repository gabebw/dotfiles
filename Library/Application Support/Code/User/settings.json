{
  "[bazel]": {
    "editor.formatOnSave": false
  },
  "[css]": {
    "editor.defaultFormatter": "prettier.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "prettier.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "prettier.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "prettier.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "prettier.prettier-vscode"
  },
  "[markdown]": {
    "editor.formatOnSave": false
  },
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "[ruby]": {
    "editor.defaultFormatter": "testdouble.vscode-standard-ruby"
  },
  "[rust]": {
    "editor.defaultFormatter": "statiolake.vscode-rustfmt"
  },
  "[scss]": {
    "editor.defaultFormatter": "prettier.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "prettier.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "prettier.prettier-vscode"
  },
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "advancedNewFile.convenienceOptions": [
    "last",
    "current",
    "root"
  ],
  "breadcrumbs.enabled": true,
  "chat.agent.codeBlockProgress": false,
  "chat.agent.enabled": false,
  "chat.checkpoints.enabled": false,
  "editor.accessibilitySupport": "off",
  "editor.autoClosingBrackets": "never",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorSurroundingLines": 5,
  "editor.fontFamily": "JetBrains Mono NL, monospace",
  "editor.fontSize": 18,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "comment",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "variable.key.table",
        "settings": {
          "foreground": "#4EC9B0"
        }
      },
      {
        "scope": "variable.key.array",
        "settings": {
          "foreground": "#569CD6"
        }
      },
      {
        "scope": "constant.other.time",
        "settings": {
          "foreground": "#DCDCAA"
        }
      }
    ]
  },
  "eslint.codeActionsOnSave.rules": [
    "unused-imports/no-unused-imports"
  ],
  "eslint.format.enable": true,
  "eslint.run": "onSave",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "files.associations": {
    "*.dev": "procfile",
    "*.env": "properties"
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.ammonite": true,
    "**/.bloop": true,
    "**/.metals": true
  },
  "git.allowForcePush": true,
  "git.autoStash": true,
  "git.confirmForcePush": false,
  "git.path": "/usr/local/bin/git",
  "git.rebaseWhenSync": true,
  "git.supportCancellation": true,
  "githubPullRequests.defaultMergeMethod": "squash",
  "gitlab.duo.enabledWithoutGitlabProject": false,
  "gitlab.duoChat.enabled": false,
  "gitlab.duoCodeSuggestions.enabled": false,
  "gitlab.keybindingHints.enabled": false,
  "gitlab.showProjectMergeRequests": false,
  "gitlens.blame.highlight.locations": [
    "gutter",
    "overview"
  ],
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.telemetry.enabled": false,
  "gitlens.views.compare.location": "gitlens",
  "gitlens.views.fileHistory.location": "explorer",
  "gitlens.views.lineHistory.location": "explorer",
  "gitlens.views.repositories.location": "scm",
  "gitlens.views.search.location": "gitlens",
  "importSorter.generalConfiguration.sortOnBeforeSave": true,
  "importSorter.importStringConfiguration.quoteMark": "double",
  "javascript.format.enable": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "lldb.suppressUpdateNotifications": true,
  "prettier.requireConfig": true,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "search.exclude": {
    "**/*.ipynb": true
  },
  "search.useGlobalIgnoreFiles": true,
  "shellcheck.executablePath": "/usr/local/bin/shellcheck",
  "stylusSupremacy.alwaysUseExtends": true,
  "stylusSupremacy.insertBraces": false,
  "stylusSupremacy.insertColons": false,
  "stylusSupremacy.insertSemicolons": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.defaultProfile.osx": "fish",
  "typescript.format.enable": false,
  "typescript.surveys.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.workspaceSymbols.scope": "currentProject",
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
  "vsicons.dontShowNewVersionMessage": true,
  "window.zoomLevel": -1,
  "workbench.colorTheme": "Witch Hazel Hypercolor",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "newUntitledFile"
}
